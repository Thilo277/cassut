<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
    <fx>
        <engine>
            <name>engine</name>
            <path>Aircraft/cassut/Sounds/gatling.wav</path>
            <mode>looped</mode>
            <condition>
                <greater-than>
                    <property>engines/engine/rpm</property>
                    <value>10</value>
                </greater-than>
            </condition>
            <volume>
                <property>/engines/engine/rpm</property>
                <factor>0.0002</factor>
                <min>0.75</min>
                <max>1.0</max>
                <offset>0.0</offset>
            </volume>
            <pitch>
                <property>engines/engine/rpm</property>
                <factor>0.001</factor>
                <min>0.0</min>
                <max>2.0</max>
                <offset>-0.5</offset>
            </pitch>
        </engine>
    </fx>
</PropertyList>