            <binding>
                <command>property-assign</command>
                <property>texter/smokeoverride</property>
                <value>0</value>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>b4</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <interval-sec>0.2</interval-sec>
            <binding>
                <command>property-assign</command>
                <property>texter/selection</property>
                <value>4</value>
            </binding>
            <binding>
                <command>property-assign</command>
                <property>texter/smokeoverride</property>
                <value>0</value>
            </binding>
        </action>
    </animation>

    <animation>
        <object-name>reds</object-name>
        <type>select</type>
        <condition>
            <equals>
                <property>texter/smokeoverride</property>
                <value>1</value>
            </equals>
        </condition>
    </animation>

    <animation>
