                <value>1</value>
            </binding>
            <binding>
                <command>property-assign</command>
                <property>texter/smokeoverride</property>
                <value>0</value>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>b2</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
