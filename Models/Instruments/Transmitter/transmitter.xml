<?xml version="1.0"?>

<PropertyList>
    <path>transmitter.ac</path>


    <animation>
        <type>pick</type>
        <object-name>bs</object-name>
        <condition>
            <equals>
                <property>texter/smokeoverride</property>
                <value>0</value>
            </equals>
        </condition>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <interval-sec>0.2</interval-sec>
            <binding>
                <command>property-assign</command>
                <property>texter/smokeoverride</property>
                <value>1</value>
            </binding>
            <binding>
                <command>nasal</command>
                <script>main.startoverride();</script>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>bs</object-name>
        <condition>
            <equals>
                <property>texter/smokeoverride</property>
                <value>1</value>
            </equals>
        </condition>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <interval-sec>0.2</interval-sec>
            <binding>
                <command>property-assign</command>
                <property>texter/smokeoverride</property>
                <value>0</value>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>b1</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <interval-sec>0.2</interval-sec>
            <binding>
                <command>property-assign</command>
